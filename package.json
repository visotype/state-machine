{
  "name": "node-elm-state",
  "version": "1.0.0",
  "description": "Use an Elm program as a state container for a JS/Node application",
  "private": true,
  "main": "index.js",
  "scripts": {
    "compile": "npx elm make src/Main.elm --output=main.js --optimize",
    "build": "npm run compile && npm run minify",
    "minify": "npx uglifyjs main.js --compress 'pure_funcs=\"F2,F3,F4,F5,F6,F7,F8,F9,A2,A3,A4,A5,A6,A7,A8,A9\",pure_getters,keep_fargs=false,unsafe_comps,unsafe' | npx uglifyjs --mangle --output=main.js",
    "sync": "git add . && git commit -m 'automated sync' && git push",
    "test": "eslint '**/*.js' && ava"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/visotype/node-elm-state"
  },
  "keywords": [],
  "author": "Daniel C. Narey",
  "license": "MIT",
  "engines": {
    "node": ">=8"
  },
  "dependencies": {},
  "devDependencies": {
    "ava": "^1.0.1",
    "elm": "^0.19.0-bugfix2",
    "eslint": "^5.10.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-ava": "^5.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-types": "^1.0.2",
    "uglify-js": "^3.4.9"
  }
}
